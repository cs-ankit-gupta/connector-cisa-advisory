    "operations": [
        {
            "operation": "get_ics_advisory",
            "title": "Get ICS Advisory",
            "annotation": "get_ics_advisory",
            "description": "Fetches all the ICS Advisory till present date or by provided date filter.",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "date_filter",
                    "title": "Date Filter",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": false,
                    "description": "Specify the date filter.",
                    "options": [
                        "Today",
                        "Last 7 Days",
                        "Last 30 Days",
                        "This Month",
                        "This Year",
                        "Last Month",
                        "Last 3 Months",
                        "Last 6 Months",
                        "Last Date Published Advisory",
                        "Custom Date"
                    ],
                    "onchange": {
                        "Custom Date": [
                            {
                                "name": "selectDate",
                                "title": "Select Date",
                                "description": "Fetches all the advisory by given date.",
                                "type": "datetime",
                                "editable": true,
                                "visible": true,
                                "required": true,
                                "value": "",
                                "tooltip": "Specify the release date or epoch time of advisory."
                            }
                        ]
                    }
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
