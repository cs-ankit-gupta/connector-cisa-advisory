{
    "name": "cisa-advisory",
    "version": "1.0.0",
    "label": "CISA Advisory",
    "description": "CISA Advisory connector fetches the Advisory published by CISA",
    "publisher": "Fortinet",
    "icon_small_name": "samll.png",
    "icon_large_name": "large.png",
    "configuration": {},
    "category": "Utilities",
    "active": true,
    "cs_compatible": true,
    "help_online": "",
    "cs_approved": false,
    "operations": [
        {
            "operation": "get_advisory",
            "title": "Get Advisory",
            "annotation": "get_advisory",
            "description": "Fetches all the Advisories present till date or by provided date filter.",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Specify the advisory Type.",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Specify the advisory type."
                },
                {
                    "name": "date_filter",
                    "title": "Date Filter",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": false,
                    "description": "Specify the date filter.",
                    "options": [
                        "Last 7 Days",
                        "Last 30 Days",
                        "This Month",
                        "This Year",
                        "Last Month",
                        "Last 3 Months",
                        "Last 6 Months",
                        "Last Date Published Advisory",
                        "Custom Date"
                    ],
                    "onchange": {
                        "Custom Date": [
                            {
                                "name": "selectDate",
                                "title": "Select Date",
                                "description": "Fetches all the advisory by given date.",
                                "type": "datetime",
                                "editable": true,
                                "visible": true,
                                "required": true,
                                "value": "",
                                "tooltip": "Specify the release date or epoch time of advisory."
                            }
                        ]
                    },
                    "tooltip": "Specify the date filter."
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": ""
            }
        },
        {
            "operation": "get_advisory_by_year",
            "title": "Get Advisory By Year",
            "annotation": "get_advisory_by_year",
            "description": "Fetches Advisories on the basis of specified year.",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Specify the advisory Type.",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Specify the advisory type.",
                    "onchange": {
                        "ICS Advisory": [
                            {
                                "title": "Year",
                                "type": "select",
                                "name": "year",
                                "required": true,
                                "visible": true,
                                "editable": true,
                                "options": [
                                    "2023",
                                    "2022",
                                    "2021",
                                    "2020",
                                    "2019",
                                    "2018",
                                    "2017",
                                    "2016",
                                    "2015",
                                    "2014",
                                    "2013",
                                    "2012",
                                    "2011",
                                    "2010"
                                ],
                                "tooltip": "Specify the ICS Advisory Year."
                            }
                        ],
                        "ICS Medical Advisory": [
                            {
                                "title": "Year",
                                "type": "select",
                                "name": "year",
                                "required": true,
                                "visible": true,
                                "editable": true,
                                "options": [
                                    "2023",
                                    "2022",
                                    "2021",
                                    "2020",
                                    "2019",
                                    "2018",
                                    "2017",
                                    "2016"
                                ],
                                "tooltip": "Specify the ICS Medical Advisory Year."
                            }
                        ]
                    }
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": ""
            }
        },
        {
